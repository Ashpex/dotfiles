set font                        "Iosevka Term 12"
set default-bg                  "#0a0f14" #00
set default-fg                  "#98d1ce" #01

set statusbar-fg                "#98d1ce"    #04
set statusbar-bg                "#0a0f14" #01

set inputbar-bg                 "#0a0f14" #00 currently not used
set inputbar-fg                 "#10151b" #02

set notification-error-bg       "#33859d" #08
set notification-error-fg       "#98d1ce" #00

set notification-warning-bg     "#26a98b" #08
set notification-warning-fg     "#98d1ce" #00

set highlight-color             "#10151b" #0A
set highlight-active-color      "#98d1ce" #0D

set completion-highlight-fg     "#d3ebe9"  #02
set completion-highlight-bg     "#4e5165" #0C

set completion-bg               "#0a0f14" #02
set completion-fg               "#98d1ce" #0C

set notification-bg             "#edb54b" #0B
set notification-fg             "#98d1ce" #00

set recolor-lightcolor          "#0a0f14" #00
set recolor-darkcolor           "#98d1ce" #06
set recolor                     "true"

# setting recolor-keep true will keep any color your pdf has.
# if it is false, itll just be black and white
set recolor-keephue             "false"

